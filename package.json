{
  "name": "ember-themed-syntax",
  "version": "0.1.1",
  "description": "An out-of-the-box solution for syntax highlighting with a couple of themes included",
  "keywords": [
    "ember-addon",
    "syntax highlighting",
    "highlightjs"
  ],
  "license": "MIT",
  "author": "Carlos Rodriguez <ideas@evolutionaryapps.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/crodriguez1a/ember-themed-syntax",
  "scripts": {
    "build": "ember build",
    "clean": "rimraf tmp/ dist/",
    "clean:all": "rimraf tmp/ dist/ node_modules/",
    "start": "ember server",
    "start:inspect": "node --inspect --debug-brk node_modules/.bin/ember server",
    "test": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^6.3.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-node-assets": "^0.2.2",
    "ember-cli-string-helpers": "^1.5.0",
    "ember-decorators": "^1.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "broccoli-asset-rev": "^2.4.5",
    "code-highlight-linenums": "^0.2.1",
    "ember-ajax": "^3.0.0",
    "ember-cli": "2.14.2",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^0.4.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-sass": "^7.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.14.1",
    "highlightjs": "^9.10.0",
    "hljs-themes": "^1.0.0",
    "loader.js": "^4.2.3",
    "rimraf": "^2.6.1"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://demos.evolutionaryapps.com/EmberThemedSyntax/"
  }
}
